.page-content-culture-item{display: table;width: 100%;border-top: 1px solid #e6e6e6;padding-bottom: 20px;padding-top: 40px;}
.page-content-culture-item:first-child{border-top: none;padding-top: 0}
.page-content-culture-item dt,.page-content-culture-item dd{display: table-cell;vertical-align: middle;}
.page-content-culture-item dt{width: 130px;}
.page-content-culture-item dt img{width: 130px;border-radius: 100%;background: #f1f6ff;}
.page-content-culture-item dd{padding-left: 70px;}
.page-content-culture-item dd table{border: none;}
.page-content-culture-item dd table td{border: none;}
.page-content-culture-item dd h3{color: #1e5e8f;}
.page-content-culture-item-detail{color: #2f3030;font-size:18px;margin-left: 10px;}
.honor-header-box{color: #646262;}
.honor-header-box *{vertical-align: middle;}
.honor-authority{margin-left: 10px;margin-right: 10px;}
.page-honer-content-body{padding-left: 100px;}
.honer-body-box{margin-top: 50px;padding-left: 30px;padding-right: 30px;}
.honer-list-item{border-left: 1px solid #d9d9d9;padding-left: 30px;padding-top: 20px;padding-bottom: 20px;}
.honer-list-item dt{color: #1e5e8f;font-size: 45px;position: relative;}
.honer-list-item dt:before {content: "";border-radius: 100%;width: 15px;height: 15px;background: #d9d9d9;position: absolute;left: -38px;top: 15px;}
.honer-list-item dd{padding-left: 160px;line-height: 30px;color: #646262;}

