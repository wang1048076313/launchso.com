<script type="text/javascript">

    var initKeFuAndGoTop = function () {
        var t = this;
        //滚动到顶部
        $(window).on("scroll", function (e) {
            var _scrollTop = $(document).scrollTop();

            $("#goTop")[_scrollTop >= 300 ? "fadeIn" : "fadeOut"]();
        })
        $("#side-bar-bar .gotop").click(function () {
            $('html,body').animate({'scrollTop': 0}, 500);
            return false;
        });
        /*关于我们侧边导航定位*/
        $(window).on('scroll',function () {
            if($(window).scrollTop()>=$('.right-nav-content').height()-$('.left-nav-content').height()){
                $('.left-nav-content').css({'display':'none'})
            }else{
                $('.left-nav-content').css({'display':'block'})
            }
        })

    };
    initKeFuAndGoTop();

        $('.left-nav-content').css({'display':'none'})


    function GetQueryString(name){
        var reg = new RegExp("(^|&)"+ name +"=([^&]*)(&|$)");
        var r = window.location.search.substr(1).match(reg);
        if(r!=null)return  decodeURI(r[2]); return null;
    };

    var newboj={id:GetQueryString('id')};
    $.ajax({
        url:'http://d.launchso.com/ebp/af/m1/m1021/site/news.do?',
        data:newboj,
        type:'get',
        dataType:'jsonp',
        success:function (data) {
            var  result='';
            result = template('test',data);
            document.getElementById('New-List').innerHTML = result;
            $('.left-nav-content').css({'display':'block'})
        }
    })
</script>



