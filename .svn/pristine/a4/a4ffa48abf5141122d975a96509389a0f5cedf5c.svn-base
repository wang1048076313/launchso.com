<script type="text/javascript">

    var initKeFuAndGoTop = function () {
        var t = this;
        //滚动到顶部
        $(window).on("scroll", function (e) {
            var _scrollTop = $(document).scrollTop();

            $("#goTop")[_scrollTop >= 300 ? "fadeIn" : "fadeOut"]();
        })
        $("#side-bar-bar .gotop").click(function () {
            $('html,body').animate({'scrollTop': 0}, 500);
            return false;
        });
        /*关于我们侧边导航定位*/
        $(window).on('scroll',function () {
            if($(window).scrollTop()>=$('.right-nav-content').height()-$('.left-nav-content').height()){
                $('.left-nav-content').css({'display':'none'})
            }else{
                $('.left-nav-content').css({'display':'block'})
            }
        })

    };
    initKeFuAndGoTop();

    /*最大页码数*/
    var max_page='';

    $('.left-nav-content').css({'display':'none'})
    function pageNos(num,fn) {
        var that=this;
        var pageno=num||1;
        var newboj={pageNo:pageno,pageSize:8};
        $.ajax({
            url:'http://d.launchso.com/ebp/af/m1/m1021/site/news/list.do?',
            data:newboj,
            type:'get',
            dataType:'jsonp',
            success:function (data) {
                 max_page=Math.ceil(data.total/8);
                var  result='';
                result = template('test',data);
                document.getElementById('New-List').innerHTML = result;

                $('.left-nav-content').css({'display':'block'})
                $('html,body').animate({'scrollTop':0},100)
                fn();
            }
        })

    }
    pageNos(null,pagsfn)
    /*
     分页插件
     */
   function pagsfn() {
       Page({
           num:max_page,					//页码数
           startnum:1,				//指定页码
           elem:$('#page1'),		//指定的元素
           callback:function(n){	//回调函数
               pageNos(n)

           }
       });
   }

</script>



