

.coop-box{margin-top: 60px}
.coop h2{font-size: 26px;color: #2f3030;border-bottom: 1px solid #b9b9b9;padding-bottom: 12px;}
.coop>p{font-size: 14px;color: #646262;padding-top:12px;letter-spacing: 1px;line-height: 22px}
.coop-tactic ul{margin-top: 36px;padding-left: 20px;padding-right: 20px;margin-left: -40px;margin-right: -40px;}
.coop-tactic ul li{width:230px ;margin-left: 39px;margin-right: 39px;display: inline-block;margin-top: 18px;cursor: pointer;border:1px solid #ccc;}
.coop-tactic ul li:hover,.coop-tactic ul li.coop-current{-webkit-box-shadow: 0 1px 10px rgba(0,0,0,0.2);-moz-box-shadow: 0 1px 10px rgba(0,0,0,0.2);-ms-box-shadow: 0 1px 10px rgba(0,0,0,0.2);}
.coop-tactic ul li .coop-cont{display: none;}
.coop-tactic .coop-copy{font-size: 14px;color: #646262;margin-top: 30px;letter-spacing: 1px;line-height: 20px;padding-left: 20px;padding-right: 20px;}
.coop-tactic ul li .coop-company{display: table-cell;vertical-align: middle;text-align: center;width: 230px;height: 74px;}
.coop-tactic ul li img{width: 100%;height: 100%;}
.coop-market{margin-top: 96px;}

.about-channel-protype-item-name{font-size: 18px;color: #2f3030;}
.about-channel-protype-item{height: 210px;overflow: hidden;}
.about-channel-protype-item-detail{font-size: 14px;color: #646262;text-align: left;}
.about-policy-protype-item-name{color: #646262;}